{
	"name": "codeigniter4/cache",
	"type": "library",
	"description": "PSR-6 and PSR-16 Cache Adapters for CodeIgniter 4",
	"keywords": [
		"codeigniter",
		"codeigniter4",
		"cache",
		"pool",
		"simplecache",
		"psr-6",
		"psr-16"
	],
	"homepage": "https://github.com/codeigniter4/cache",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew Gatner",
			"email": "mgatner@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.3 || ^8.0",
		"psr/cache": "^1.0",
		"psr/simple-cache": "^1.0"
	},
	"provide": {
		"psr/cache-implementation": "^1.0",
		"psr/simple-cache-implementation": "^1.0"
	},
	"require-dev": {
		"cache/integration-tests": "^0.17.0",
		"codeigniter4/codeigniter4": "dev-develop",
		"codeigniter4/codeigniter4-standard": "^1.0",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5",
		"squizlabs/php_codesniffer": "^3.3"
	},
	"autoload": {
		"psr-4": {
			"CodeIgniter\\Psr\\Cache\\": "src"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codeigniter4/CodeIgniter4"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"analyze": "phpstan analyze",
		"style": "phpcbf --standard=./vendor/codeigniter4/codeigniter4-standard/CodeIgniter4 tests/ src/",
		"test": "phpunit"
	}
}
